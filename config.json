    "useSessionWindows": false,
    "killzones": "02:00-05:00,08:30-10:00,13:30-15:00",
    "firstMinGuard": 1,
    "lastMinGuard": 1,
    "cashSessionGuard": false,

    "lookback": 250,
    "imbalanceLookback": 30,
    "sweepTolBps": 2.0,
    "minAtrBps": 3,
    "pdToleranceBps": 30,

    "preferIFVG": true,
    "requireMicroBOS": true,
    "requireSweep": true,

    "usePD": true,
    "useOTE": true,
    "oteLo": 0.58,
    "oteHi": 0.79,

    "bias": {
      "enabled": true,
      "gate": "strict",
      "emaPeriod": 50,
      "slopeBps": 1,
      "htf4h": 240,
      "htf1h": 60,
      "fallback15m": 15,
      "bandBps": 8
