    "entryMode": "edge",
    "entryExpiryBars": 6,
    "rr": 2.0,
    "atrPeriod": 14,
    "atrMult": 1.1,
    "minStopBps": 6,
    "breakevenAtR": 1.0,
    "trailAfterR": 1.6,
    "cooldownBars": 8,

    "fvgMinBps": 2.5,
    "minBodyAtr": 0.4
