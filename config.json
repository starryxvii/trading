{
  "strategy": "ultra",
  "symbol": "BTC-USD",
  "interval": "5m",
  "period": "60d",

  "equity": 10000,
  "riskPct": 0.75,
  "rr": 2.0,

  "session": "AUTO",
  "debug": false,
  "report": { "exportCsv": true },
  "maxConcurrentTrades": 1,
  "pollMs": 15000,

  "slippageBps": 1,
  "feeBps": 0,

  "scaleOutAtR": 0.0,
  "scaleOutFrac": 0.00,
  "finalTP_R": 3.2,

  "maxDailyLossPct": 1.5,

  "atrTrailMult": 0.0,
  "atrTrailPeriod": 14,

  "mfeTrail": { "enabled": true, "armR": 1.6, "givebackR": 0.35 },

  "pyramiding": { "enabled": false },

